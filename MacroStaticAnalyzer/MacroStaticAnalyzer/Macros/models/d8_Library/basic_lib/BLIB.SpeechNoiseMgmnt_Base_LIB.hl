				}
				else
				{
					hi:d8MFnyBand[0] = 5;
					hi:d8MVadBand[0] = 5;
					if (hi:d8PhlNruSpassMode == d8PhlNruSpassMode.FANCY_auto_nf )
					{
						hi:d8PhlNruSpassMode = d8PhlNruSpassMode.FANCY;					

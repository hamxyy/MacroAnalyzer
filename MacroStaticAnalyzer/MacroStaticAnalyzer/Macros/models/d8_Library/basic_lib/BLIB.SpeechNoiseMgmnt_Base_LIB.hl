    public function bool SpeechNoiseMgmntEn_Get()
    {      	
    	if (hi:d9FNruVadEn == d9FNruVadEn.on)
    	{	
    		println("[HAL]BLIB:SpeechNoiseMgmntEn_Get = true ");
    		return true;	
